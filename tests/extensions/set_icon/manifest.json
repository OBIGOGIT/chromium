{
  "name": "Icon Example",
  "description": "Demonstrates usage and features of browserAction.setIcon",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["alarms", "tabs", "bookmarks", "declarativeWebRequest", "*://*/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon" : "icon.png",
    "default_title": "Change icon"
  }
}
