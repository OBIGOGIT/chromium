{
  "name": "<webview>.executeScript after contentload",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "30",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "webview"
  ],
  "webview": {
    "partitions": [
      {
        "name": "privileged",
        "accessible_resources": ["*"]
      }
    ]
  }
}
